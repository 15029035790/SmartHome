    app:sCenterBottomTextSize="@dimen/text_size_medium"
    app:sCenterViewGravity="left_center"
    app:sDividerLineHeight="1dp"
    app:sDividerLineColor="@color/stv_divider"
    app:sBackgroundDrawableRes="@drawable/device_item_background"
