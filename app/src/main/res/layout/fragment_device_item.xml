    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/device_item"
    android:layout_width="match_parent"
    android:layout_height="@dimen/stv_double_layout_height"
    app:sLeftTopTextSize="@dimen/text_size_large"
